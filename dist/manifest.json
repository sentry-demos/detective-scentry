{
  "name": "Scentry",
  "version": "0.0.4",
  "manifest_version": 3,
  "description": "Finds Sentry and other SDKs",
  "icons": {
    "16": "icons/santo-16.png",
    "32": "icons/santo-32.png",
    "64": "icons/santo-64.png"
  },
  "default_locale": "en",
  "action": {
    "default_title": "Santo SDK Detector",
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "storage", "activeTab", "tabs"],
  "web_accessible_resources": [
    {
      "resources": ["js/*", "content.js", "*.map", "*.png", "*.jpg"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["https://*/*", "http://*/*"]
}
