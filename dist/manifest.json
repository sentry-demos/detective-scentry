{
  "name": "Scentry",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Finds Sentry and other SDKs",
  "icons": {
    "16": "icons/santo-16.png",
    "32": "icons/santo-32.png",
    "64": "icons/santo-64.png"
  },
  "web_accessible_resources": ["js/*", "content.js", "*.map", "*.png", "*.jpg"],
  "browser_action": {
    "default_title": "Santo SDK Detector",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "permissions": ["https://*/*", "activeTab", "tabs", "<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
